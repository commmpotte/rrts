yle={{ display: 'flex' }} className=\"d-flex\">\n        {pages.map((p) => (\n          <li\n            className=\"list-group-item p-2 m-2\"\n            key={p}\n            onClick={() => setTodoPage(p)}\n            style={{\n              cursor: 'pointer',\n              background: p === page ? 'blue' : 'green',\n              color: 'white',\n            }}\n          >\n            {p}\n          </li>\n        ))}\n      </div>\n    </>\n  )\n}\n\nexport default TodoList\n","/Users/commmpotte/FrontEnd/Extra_Learning/ulbi_ts_rd_react/src/components/UserList.tsx",["85","86","87"],[],"import React, { useEffect } from 'react'\nimport { useTypedSelector } from '../hooks/useTypedSelector'\nimport { useDispatch } from 'react-redux'\nimport { fetchUsers } from '../store/action-creators/user'\nimport { useActions } from '../hooks/useActions'\n\nconst UserList: React.FC = () => {\n  const { users, error, loading } = useTypedSelector((state) => state.user)\n  const { fetchUsers } = useActions()\n\n  useEffect(() => {\n    fetchUsers()\n  }, [])\n\n  if (loading) {\n    return <h1>Loading...</h1>\n  }\n\n  if (error) {\n    return <h1>{error}</h1>\n  }\n\n  return (\n    <>\n      <h1>User List</h1>\n      <ul className=\"list-group text-left\">\n        {users.map((user) => (\n          <li className=\"list-group-item mb-2\" key={user.id}>\n            {user.name}\n          </li>\n        ))}\n      </ul>\n    </>\n  )\n}\n\nexport default UserList\n","/Users/commmpotte/FrontEnd/Extra_Learning/ulbi_ts_rd_react/src/store/index.ts",[],[],"/Users/commmpotte/FrontEnd/Extra_Learning/ulbi_ts_rd_react/src/hooks/useActions.ts",[],[],"/Users/commmpotte/FrontEnd/Extra_Learning/ulbi_ts_rd_react/src/hooks/useTypedSelector.ts",[],[],"/Users/commmpotte/FrontEnd/Extra_Learning/ulbi_ts_rd_react/src/store/reducer/index.ts",["88"],[],"import { combineReducers, createStore } from 'redux'\nimport { userReducer } from './userReducer'\nimport { todoReducer } from './todoReducer'\n\nexpo